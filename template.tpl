// Saisissez le code de votre modèle ici.
const log = require('logToConsole');
const getCookieValues = require('getCookieValues');
const setDefaultConsentState = require('setDefaultConsentState');
const updateConsentState = require('updateConsentState');

// Process default consent state
if (data.command === 'default') {
  setDefaultConsentState({
  'ad_storage': 'denied',
  'analytics_storage': 'denied',
  'wait_for_update': 2000
});

let consentSettings = {
  ad_storage: 'denied',
  analytics_storage: 'denied'
};
let axeptioCookie = getCookieValues('axeptio_authorized_vendors');

if(axeptioCookie){
  let cookieTable = axeptioCookie[0].split(',');
  cookieTable.forEach(x => {
    if(x === 'google_analytics'){
      consentSettings.analytics_storage = 'granted';
    }
    if(x === 'google_ads'){
      consentSettings.ad_storage = 'granted';
    }
  });
  updateConsentState(consentSettings);
}
}

// Process updated consent state
if (data.command === 'update') {
  let consentSettings = {
  ad_storage: 'denied',
  analytics_storage: 'denied'
};
let axeptioCookie = getCookieValues('axeptio_authorized_vendors');

if(axeptioCookie){
  let cookieTable = axeptioCookie[0].split(',');
  cookieTable.forEach(x => {
    if(x === 'google_analytics'){
      consentSettings.analytics_storage = 'granted';
    }
    if(x === 'google_ads'){
      consentSettings.ad_storage = 'granted';
    }
  });
  updateConsentState(consentSettings);
}
}

// Call data.gtmOnSuccess when the tag is finished.
data.gtmOnSuccess();


